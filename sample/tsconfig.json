{
  "compilerOptions": {
    "module": "es6",
    "removeComments": true,
    "pretty": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "target": "es6",
    "moduleResolution": "node",
    "noEmit": false,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "ququmber-ui/*": ["../src/*"],
      "listlab-api/*": ["../node_modules/listlab-api/src/*"],
      "*": ["../node_modules/*"]
    },
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "jsxImportSource": "@emotion/react",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "../node_modules"
  ]
}
