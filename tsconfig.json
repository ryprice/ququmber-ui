{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "target": "es6",
    "moduleResolution": "node",
    "noEmit": false,
    "jsx": "react",
    "baseUrl": ".",
    "rootDir": "./src/",
    "paths": {
      "ququmber-ui/*": ["src/*"],
      "ququmber-api/*": ["node_modules/ququmber-api/src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
